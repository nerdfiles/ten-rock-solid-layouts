/* ========================================================================== iPad == */

/* Portait and Landscape */

@media only screen and (min-device-width: 768px) and (max-device-width: 1024px) {

  body { } 
  
} /* End Portrait and Landscape */

/* Portrait */

@media only screen and (orientation:portrait) and (min-device-width: 768px) and (max-device-width: 1024px) {

  body { } 
  
} /* End Portrait */

/* Landscape */

@media only screen and (orientation:landscape) and (min-device-width: 768px) and (max-device-width: 1024px) {

  body._template-name_threeboxes { 
    
    .container { 
      width: 90%;
      margin: 0 5%;
      .header-base { 
        //@include grid-(3);
        @include center-grid;
        @include grid-container;
        width: 100%;
        margin-top: 20px;
        margin-bottom: 20px;
        .logo { 
          @include grid-(1); 
          }
        .nav { 
          @include grid-(3); 
          float: right;
          ul { float: right; } } 
      }
      
      .content-base { 
        .box { 
          @include center-grid; } 
        .box-stage { 
          //@include grid-(3);
          //@include center-grid;
          width: 100%;
          margin-bottom: 20px; }
        .box-row { 
          .box-stage { 
            //@include grid-(3);
            @include center-grid; 
            width: 100%;
            margin-bottom: 20px; } } } 
            
      .footer-base { 
        //@include grid-(3);
        @include center-grid;
        width: 100%;
        margin-bottom: 20px; } } }
         
} /* End Landscape */
   
/* ========================================================================== iPhone == */

/* Portait and Landscape */

@media only screen and (max-device-width: 480px), (max-width: 480px) {

  body { background: orange; } 
  
} /* End Portrait and Landscape */

/* Portait */

@media only screen and (orientation:portrait) and (max-device-width: 480px) and (max-width: 480px) {
    
/* ==================================================================================
